{
  "name": "symbiote/silverstripe-listingpage",
  "description": "A module that allows CMS users to create templated listing pages.",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "pagetype",
    "template"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Symbiote",
      "homepage": "https://www.symbiote.com.au/"
    },
    {
      "name": "Marcus Nyeholt",
      "email": "marcus@symbiote.com.au"
    }
  ],
  "require": {
    "silverstripe/framework": "~4.0",
    "symbiote/silverstripe-multivaluefield": "~5.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Symbiote\\ListingPage\\": "src/",
      "Symbiote\\ListingPage\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.2.x-dev"
    },
    "expose": [
      "client/images"
    ]
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
